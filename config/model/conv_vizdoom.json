{
  "name" : "vizdoom_default",
  "algo" : "dqn",
  "device" : "cuda",

  "dump_log_every" : 2000,

  "algo_params": {

    "architecture" : "conv",
    "architecture_params" : {
      "ignore_text" : false,
      "use_lstm_after_conv" : false,
      "rnn_text_hidden_size" : 128,
      "fc_text_embedding_hidden" : 32,
      "last_hidden_fc_size" : 64,
      "dueling_architecture" : true,
      "learn_text" : true,
      "use_memory" : false,
      "conv_layers_channel" : [25, 50, 75],
      "conv_layers_size" : [8,4,4],
      "conv_layers_stride" : [4,2,1],
      "max_pool_layers" : [0,0,0]

    },

    "n_parallel_env" : 1,
    "batch_size" : 128,
    "lr" : 1e-5,
    "replay_buffer_size" : 40000,
    "gamma" : 0.99,
    "update_target_every" : 2000,
    "step_exploration" : 20000,
    "weight_decay" : 0
  },

  "wrappers_model" : [
    {"name" : "MinigridTorchWrapper", "params" : {"device" : "cuda"}}
  ]

}



