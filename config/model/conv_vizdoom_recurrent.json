          "max_pool_layers" : [2,0,0],
          "embedding_dim" : 32,
          "generator_max_len" : 10

        }
      }
    },

    "n_parallel_env" : 1,
    "batch_size" : 128,
    "lr" : 1e-5,
    "n_step" : 1,
    "gamma" : 0.99,
    "update_target_every" : 20000,
    "step_exploration" : 40000,
    "weight_decay" : 0
  },

  "wrappers_model" : [
    {"name" : "MinigridTorchWrapper", "params" : {"device" : "cuda"}}
  ]

