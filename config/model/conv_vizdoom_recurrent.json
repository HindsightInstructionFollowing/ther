{
  "name" : "minigrid_fetch_rdddqn_proj_bigger_gated",
  "algo" : "rdqn",
  "device" : "cuda",

  "dump_log_every" : 30000,

  "algo_params": {
    "architecture" : "conv_lstm",

    "architecture_params" : {
      "conv_layers_channel" : [25, 50, 75],
      "conv_layers_size" : [8,4,4],
      "conv_layers_stride" : [4,2,2],
      "max_pool_layers" : [0,0,0],
      "projection_after_conv" : 1024,

      "use_gated_attention" : true,

      "ignore_text" : false,
