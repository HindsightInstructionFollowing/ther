{
  "name" : "minigrid_no_text_dueling",
  "algo" : "dqn",
  "device" : "cuda",

  "algo_params": {

    "architecture" : "conv",
    "architecture_params" : {
      "ignore_text" : true,
      "use_lstm_after_conv" : false,
      "rnn_text_hidden_size" : 128,
      "fc_text_embedding_hidden" : 32,
      "last_hidden_fc_size" : 64,
      "dueling_architecture" : true
    },

    "batch_size" : 128,
    "lr" : 5e-4,
    "replay_buffer_size" : 40000,
    "gamma" : 0.99,
    "update_target_every" : 2000,
    "step_exploration" : 20000,
    "weight_decay" : 0,
    "n_stack_frame" : 1
  },

  "wrappers_model" : [
    {"name" : "RemoveUselessChannelWrapper", "params" : {}},
    {"name" : "RemoveUselessActionWrapper", "params" : {}},
    {"name" : "MinigridTorchWrapper", "params" : {"device" : "cuda"}}
  ]



}

