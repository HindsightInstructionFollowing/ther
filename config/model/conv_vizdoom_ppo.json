{
  "name" : "vizdoom_default_ppo_parallel_12",
  "algo" : "ppo",
  "device" : "cuda",

  "dump_log_every" : 2000,

  "algo_params": {

    "architecture" : "conv",
    "architecture_params" : {
      "ignore_text" : false,
      "dueling_architecture" : false,
