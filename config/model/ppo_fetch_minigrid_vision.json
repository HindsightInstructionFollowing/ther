{
  "name" : "minigridfetch_ppo_memory_vision_objective_distract_bigger",
  "algo" : "ppo",
  "device" : "cuda",

  "dump_log_every" : 10000,

  "algo_params": {

    "architecture" : "conv",
    "architecture_params" : {
      "dueling_architecture" : false,
      "use_lstm_after_conv" : false,
      "rnn_text_hidden_size" : 128,
      "fc_text_embedding_hidden" : 32,
      "last_hidden_fc_size" : 128,
      "ignore_text" : true,
      "use_memory" : true,
      "conv_layers_channel" : [25, 50, 75]

    },

    "n_parallel_env" : 12,
    "num_frames_per_proc" : 128,
    "gae_lambda" : 0.95,
    "entropy_coef" : 0.01,
    "value_loss_coef" : 0.5,
    "max_grad_norm" : 0.5,
    "recurrence" : 4,
    "gamma" : 0.99,

    "lr" : 1e-3,
    "adam_eps" : 1e-8,
    "epochs" : 4,
    "batch_size" : 256,
    "clip_eps" : 0.2

  },

  "wrappers_model" : [
    {"name" : "VisionObjectiveWrapper", "params" :  {"with_distractor" : true}},
    {"name" : "RemoveUselessChannelWrapper", "params" : {}},
    {"name" : "RemoveUselessActionWrapper", "params" : {}},
    {"name" : "MinigridTorchWrapper", "params" : {"device" : "cuda"}}
  ]

}



